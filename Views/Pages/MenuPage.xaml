<Page
    x:Class="Kindergarten.Views.Pages.MenuPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    Title="MenuPage"
    HorizontalAlignment="Center"
    VerticalAlignment="Center"
    d:DesignHeight="210"
    d:DesignWidth="300"
    Loaded="MenuPage_OnLoaded"
    mc:Ignorable="d">

    <StackPanel Width="300">
        <Button
            Name="BtnClubsMove"
            Margin="0,5"
            Click="BtnClubsMove_OnClick"
            Content="Группы" />
        <Button
            Name="BtnChildrenMove"
            Margin="0,5"
            Click="BtnChildrenMove_OnClick"
            Content="Дети" />
        <Button
            Name="BtnParentsMove"
            Margin="0,5"
            Click="BtnParentsMove_OnClick"
            Content="Родители" />
        <Button
            Name="BtnUsersMove"
            Margin="0,5"
            Click="BtnUsersMove_OnClick"
            Content="Пользователи" />
    </StackPanel>
</Page>
